--  DDL for Index PREFECTURE_PK
--------------------------------------------------------

  CREATE UNIQUE INDEX "PREFECTURE_PK" ON "PREFECTURE" ("PREFECTURE_ID") 
  PCTFREE 10 INITRANS 20 MAXTRANS 255 COMPUTE STATISTICS 
  STORAGE(INITIAL 65536 NEXT 1048576 MINEXTENTS 1 MAXEXTENTS 2147483645
  PCTINCREASE 0 FREELISTS 1 FREELIST GROUPS 1
  BUFFER_POOL DEFAULT FLASH_CACHE DEFAULT CELL_FLASH_CACHE DEFAULT)
  TABLESPACE "DATA" ;
--------------------------------------------------------
--  DDL for Trigger PREFECTURE_INSERT_TRG
--------------------------------------------------------

  CREATE OR REPLACE EDITIONABLE TRIGGER "PREFECTURE_INSERT_TRG" 
BEFORE INSERT ON prefecture
FOR EACH ROW
DECLARE
  l_login_id VARCHAR2(100);
BEGIN
  :new.prefecture_id := prefecture_prefecture_id_seq.nextval;

  l_login_id := apex_util.get_session_state('G_LOGIN_ID');
  IF l_login_id IS NULL THEN
    l_login_id := 'mitoco';
  END IF;

  IF :new.create_user_id = 'mitoco' THEN
    :new.create_user_id := l_login_id;
  END IF;
  IF :new.update_user_id = 'mitoco' THEN
    :new.update_user_id := l_login_id;
  END IF;

END;
    
/
ALTER TRIGGER "PREFECTURE_INSERT_TRG" ENABLE;
--------------------------------------------------------
--  DDL for Trigger PREFECTURE_UPDATE_TRG
--------------------------------------------------------

  CREATE OR REPLACE EDITIONABLE TRIGGER "PREFECTURE_UPDATE_TRG" 
BEFORE UPDATE ON prefecture
FOR EACH ROW
DECLARE
  l_login_id varchar2(100);
BEGIN
  IF not(updating('version')) THEN
    :new.version := :old.version + 1;
  END IF;

  l_login_id := apex_util.get_session_state('G_LOGIN_ID');
  IF l_login_id IS NULL THEN
    l_login_id := 'mitoco';
  END IF;

  IF not(updating('update_user_id')) THEN
    :new.update_user_id := l_login_id;
  END IF;

  IF not(updating('update_date')) THEN
    :new.update_date := sysdate;
  END IF;
END;
    
/
ALTER TRIGGER "PREFECTURE_UPDATE_TRG" ENABLE;
--------------------------------------------------------
--  Constraints for Table PREFECTURE
--------------------------------------------------------

  ALTER TABLE "PREFECTURE" MODIFY ("PREFECTURE_ID" NOT NULL ENABLE);
  ALTER TABLE "PREFECTURE" MODIFY ("PREFECTURE_NAME" NOT NULL ENABLE);
  ALTER TABLE "PREFECTURE" MODIFY ("VERSION" NOT NULL ENABLE);
  ALTER TABLE "PREFECTURE" MODIFY ("DELETE_FLAG" NOT NULL ENABLE);
  ALTER TABLE "PREFECTURE" MODIFY ("CREATE_USER_ID" NOT NULL ENABLE);
  ALTER TABLE "PREFECTURE" MODIFY ("CREATE_DATE" NOT NULL ENABLE);
  ALTER TABLE "PREFECTURE" MODIFY ("UPDATE_USER_ID" NOT NULL ENABLE);
  ALTER TABLE "PREFECTURE" MODIFY ("UPDATE_DATE" NOT NULL ENABLE);
  ALTER TABLE "PREFECTURE" ADD CHECK ( delete_flag IN ( '0', '1' ) ) ENABLE;
  ALTER TABLE "PREFECTURE" ADD CONSTRAINT "PREFECTURE_PK" PRIMARY KEY ("PREFECTURE_ID")
  USING INDEX PCTFREE 10 INITRANS 20 MAXTRANS 255 COMPUTE STATISTICS 
  STORAGE(INITIAL 65536 NEXT 1048576 MINEXTENTS 1 MAXEXTENTS 2147483645
  PCTINCREASE 0 FREELISTS 1 FREELIST GROUPS 1
  BUFFER_POOL DEFAULT FLASH_CACHE DEFAULT CELL_FLASH_CACHE DEFAULT)
  TABLESPACE "DATA"  ENABLE;
