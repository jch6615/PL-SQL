--  DDL for Table PREFECTURE
--------------------------------------------------------

  CREATE TABLE "PREFECTURE" 
   (	"PREFECTURE_ID" NUMBER, 
	"PREFECTURE_NAME" VARCHAR2(100 CHAR) COLLATE "USING_NLS_COMP", 
	"DISPLAY_ORDER" NUMBER, 
	"VERSION" NUMBER DEFAULT 0, 
	"DELETE_FLAG" VARCHAR2(1 CHAR) COLLATE "USING_NLS_COMP" DEFAULT '0', 
	"CREATE_USER_ID" VARCHAR2(100 CHAR) COLLATE "USING_NLS_COMP" DEFAULT 'mitoco', 
	"CREATE_DATE" DATE DEFAULT sysdate, 
	"UPDATE_USER_ID" VARCHAR2(100 CHAR) COLLATE "USING_NLS_COMP" DEFAULT 'mitoco', 
	"UPDATE_DATE" DATE DEFAULT sysdate
   )  DEFAULT COLLATION "USING_NLS_COMP" SEGMENT CREATION IMMEDIATE 
  PCTFREE 10 PCTUSED 40 INITRANS 10 MAXTRANS 255 
 NOCOMPRESS LOGGING
  STORAGE(INITIAL 65536 NEXT 1048576 MINEXTENTS 1 MAXEXTENTS 2147483645
  PCTINCREASE 0 FREELISTS 1 FREELIST GROUPS 1
  BUFFER_POOL DEFAULT FLASH_CACHE DEFAULT CELL_FLASH_CACHE DEFAULT)
  TABLESPACE "DATA" ;

   COMMENT ON COLUMN "PREFECTURE"."PREFECTURE_ID" IS '都道府県ID：都道府県の主キー';
   COMMENT ON COLUMN "PREFECTURE"."PREFECTURE_NAME" IS '都道府県名';
   COMMENT ON COLUMN "PREFECTURE"."DISPLAY_ORDER" IS '表示順序：画面に一覧表示する際の並び順（昇順）';
   COMMENT ON COLUMN "PREFECTURE"."VERSION" IS 'バージョン';
   COMMENT ON COLUMN "PREFECTURE"."DELETE_FLAG" IS '削除フラグ：1:論理削除した（無効）、0:論理削除していない（有効）';
   COMMENT ON COLUMN "PREFECTURE"."CREATE_USER_ID" IS '作成ユーザーID：レコードを作成したユーザーID';
   COMMENT ON COLUMN "PREFECTURE"."CREATE_DATE" IS '作成日時：レコードを作成した日時';
   COMMENT ON COLUMN "PREFECTURE"."UPDATE_USER_ID" IS '更新ユーザーID：レコードの最終更新ユーザーID';
   COMMENT ON COLUMN "PREFECTURE"."UPDATE_DATE" IS '更新日時：レコードを最終更新した日時';
   COMMENT ON TABLE "PREFECTURE"  IS '都道府県';
